{
    "name": "panda/panda",
    "description": "The Panda framework application.",
    "keywords": [
        "framework",
        "panda"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "panda/framework": "^2.0",
        "panda/ui": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
